{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build",
            "config": {
                "outputDirectory": "build/web"
            }
        }
    ],
    "installCommand": "flutter config --enable-web && flutter pub get",
    "buildCommand": "flutter build web --release",
    "outputDirectory": "build/web",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "github": {
        "silent": true
    }
} 